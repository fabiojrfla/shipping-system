<%= form_with model: @vehicle do |f| %>
  <div>
    <%= f.label :license_plate %>
    <%= f.text_field :license_plate %>
  </div>
  <div>
    <%= f.label :make %>
    <%= f.text_field :make %>
  </div>
  <div>
    <%= f.label :model %>
    <%= f.text_field :model %>
  </div>
  <div>
    <%= f.label :year %>
    <%= f.number_field :year %>
  </div>
  <div>
    <%= f.label :max_load %>
    <%= f.text_field :max_load %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
