<%= form_with model: @shipping_company do |f| %>
  <div>
    <%= f.label :registration_number %>
    <%= f.number_field :registration_number %>
  </div>
  <div>
    <%= f.label :corporate_name %>
    <%= f.text_field :corporate_name %>
  </div>
  <div>
    <%= f.label :brand_name %>
    <%= f.text_field :brand_name %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :street_name %>
    <%= f.text_field :street_name %>
  </div>
    <div>
    <%= f.label :street_number %>
    <%= f.number_field :street_number %>
  </div>
    <div>
    <%= f.label :complement %>
    <%= f.text_field :complement %>
  </div>
    <div>
    <%= f.label :district %>
    <%= f.text_field :district %>
  </div>
  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :postal_code %>
    <%= f.number_field :postal_code %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
